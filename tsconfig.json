{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "esnext",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "allowJs": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules", "dist",
  ],
  "references": [
    { "path": "./packages/core" },
    { "path": "./packages/svgo-optimize" },
    { "path": "./packages/transform-to-jsx" },
    { "path": "./packages/parse-properties" },
  ]
}
